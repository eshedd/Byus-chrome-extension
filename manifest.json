{
    "manifest_version": 2,
   
    "name": "Python Chrome Plugin",
    "description": "This extension runs Python code.",
    "version": "1.0",
   
    "browser_action": {
      "default_icon": "man.png",
      "default_popup": "popup.html"
    },
    "permissions": [
      "activeTab",
      "https://ajax.googleapis.com/", 
        "http://127.0.0.1:5000/*"
    ],
  "content_security_policy": "script-src 'self' http://127.0.0.1:5000/*; object-src 'self'"
    
}